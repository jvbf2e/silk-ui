#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules:/mnt/e/jvbf2e/node_modules:/mnt/e/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/acorn/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/acorn/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules/acorn/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/acorn@8.7.0/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules:/mnt/e/jvbf2e/node_modules:/mnt/e/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/acorn/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/acorn/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../acorn/bin/acorn" "$@"
else
  exec node  "$basedir/../acorn/bin/acorn" "$@"
fi
