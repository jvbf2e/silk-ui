#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules:/mnt/e/jvbf2e/node_modules:/mnt/e/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.5.0/node_modules/eslint-utils/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.5.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/bin/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules/eslint/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint@8.5.0/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules:/mnt/e/jvbf2e/node_modules:/mnt/e/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.5.0/node_modules/eslint-utils/node_modules:/mnt/e/jvbf2e/silk-ui/node_modules/.pnpm/eslint-utils@3.0.0_eslint@8.5.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
else
  exec node  "$basedir/../../../../../eslint@8.5.0/node_modules/eslint/bin/eslint.js" "$@"
fi
