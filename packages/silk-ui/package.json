{
  "name": "silk-ui",
  "version": "1.0.0",
  "description": "A Component Library for Vue 3",
  "license": "MIT",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.mjs"
    },
    "./es": "./es/index.mjs",
    "./lib": "./lib/index.js",
    "./*": "./*"
  },
  "main": "lib/index.js",
  "module": "es/index.mjs",
  "style": "dist/index.css",
  "unpkg": "dist/index.full.js",
  "jsdelivr": "dist/index.full.js",
  "sideEffects": [
    "dist/*",
    "theme-chalk/*.css",
    "theme-chalk/src/*.scss",
    "es/components/*/style/*",
    "lib/components/*/style/*",
    "lib/components/**/*",
    "es/components/**/*"
  ],
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "browserslist": [
    "> 1%",
    "not ie 11",
    "not op_mini all"
  ]
}
